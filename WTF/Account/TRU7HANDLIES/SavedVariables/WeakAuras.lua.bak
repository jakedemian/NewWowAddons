ot r.count then\n    r.count = r.bar:CreateFontString(nil, \"OVERLAY\")\n    r.count:SetJustifyH(\"CENTER\")\n    r.count:SetJustifyV(\"MIDDLE\")\n    r.count:SetPoint(\"CENTER\", r.bar, \"CENTER\", 0, 0)\n    r.count:SetFont(r.text:GetFont())\n    r.count:SetTextColor(r.text:GetTextColor())\n    r.count:Show()\n    r.count:SetFont(r.text:GetFont())\n    r.count:SetTextColor(r.text:GetTextColor())\n    r.count:SetShadowOffset(1, -1)\nend\naura_env.powerTextUpdate = function()\n    --print(1)\n    local a = aura_env\n    local r = a.region\n    --print(a.power)\n    if a.powerToken == \"MANA\" then \n        if a.power == a.powerMax then\n            return \"100%\"\n        else\n            return string.format(\"%.1f\", (a.power/a.powerMax)*100)..\"%\"\n        end\n    else\n        if a.castPower > 0 then\n            if a.power + a.castPower > a.powerMax then\n                return \"|cFFFF0000*|r\"..a.power..\"+\"..a.castPower..\"|cFFFF0000*|r\"\n            else\n                return a.power..\"+\"..a.castPower\n            end\n        else\n            return a.power\n        end\n    end\nend\naura_env.powerTextUpdate()",
